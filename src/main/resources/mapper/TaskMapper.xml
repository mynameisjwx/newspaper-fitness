<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hand6.demo.infra.mapper.TaskMapper">

    <select id="selectByTaskNumber" resultType="com.hand6.demo.domain.entity.Task">
        SELECT *
        FROM TODO_TASK
        WHERE TASK_NUMBER = #{taskNumber}
    </select>

</mapper>